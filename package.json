{
  "name": "@steampunk/sfdx-steampunk",
  "description": "Steampunk's SFDX Plugin",
  "version": "0.1.4",
  "author": "Steampunk",
  "license": "MIT",
  "bugs": "https://github.com/SteampunkFoundry/sfdx-steampunk/issues",
  "homepage": "https://github.com/SteampunkFoundry/sfdx-steampunk",
  "repository": "SteampunkFoundry/sfdx-steampunk",
  "keywords": [
    "sfdx-plugin"
  ],
  "bin": {
    "sfdx": "bin/run"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "oclif": {
    "bin": "sfdx",
    "commands": "./lib/commands",
    "plugins": [
      "@steampunk/sfdx-steampunk-data"
    ],
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "dependencies": {
    "@oclif/command": "^1.8.16",
    "@oclif/config": "^1.18.3",
    "@oclif/errors": "^1.3.5",
    "@steampunk/sfdx-steampunk-data": "^0.1.4",
    "tslib": "^2.0.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/plugin-help": "^3.2.0",
    "@oclif/test": "^1.2.8",
    "@salesforce/dev-config": "^2.0.0",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.13",
    "chai": "^4.2.0",
    "globby": "^11.0.1",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "sinon": "^9.2.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "lint": "tslint --project . --config tslint.json --format stylish",
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
